    "version": "0.2.0",
    "configurations": [
        {
            "name": "Framer",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/framer.exe",
            "args": [],
            "MIMode": "gdb",
            "stopAtEntry": false,
            "miDebuggerPath": "gdb.exe",
            "externalConsole": false,
            "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": false
              }
          ],
          
          "preLaunchTask": "Pre-run build"
        }

    ]
